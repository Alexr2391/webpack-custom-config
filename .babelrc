{
    "presets": [
        "@babel/preset-env",
        "@babel/preset-react",
        "@babel/preset-typescript"
    ],
    "plugins": [
        "react-refresh/babel",
        "react-require",
        [
            "moduleResolver",
            {
                "alias": {
                    "@utils": "./src/utils",
                    "@components": "./src/components",
                    "@common": "./src/common",
                    "@constants": "./src/constants"
                }
            }
        ]
    ]
}